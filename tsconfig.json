{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
  },
  "include": ["./public/main.js", "./generate.js"]
}
